#### Ontology Metadata

och: a owl:Ontology ;
    cc:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
    dcterms:bibliographicCitation "Conde-Herreros, D., Stork, L., Pernisch, R., Poveda-Villalón, M., Corcho, O., & Chaves-Fraga, D. (2024). Propagating Ontology Changes to Declarative Mappings in Construction of Knowledge Graphs." ;
    dcterms:contributor "Diego Conde Herreros",
        "Raúl Palma" ;
    dcterms:created "2024-04-17"^^xsd:date ;
    dcterms:modified "2024-12-03"^^xsd:date;    
    dcterms:creator "David Chaves-Fraga",
        "Diego Conde-Herreros",
        "Lise Stork",
        "María Poveda-Villalón",
        "Oscar Corcho",
        "Raúl Palma",
        "Romana Pernisch" ;
    dcterms:description "An ontology for describing changes between OWL ontology versions" ;
    dcterms:title "OWL Change Ontology" ;
    vann:preferredNamespacePrefix "och" ;
    vann:preferredNamespaceUri <https://w3id.org/def/och#> ;
    owl:priorVersion <https://w3id.org/def/och#> ;
    owl:versionIRI <http://w3id.org/def/och/2.0.0> ;
    owl:versionInfo "2.0.0"@en;
    mod:createdWith <https://chowlk.linkeddata.es/> .

############################################################
#### SKOS for the representation ###
###########################################################
och: a skos:ConceptScheme.

och:PropertyCharacteristic a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Property Characteristic"@en;
        skos:definition "The top term that encompases all characteristics"@en;
        skos:narrower och:FunctionalProperty, och:ObjectPropertyCharacteristic.

och:FunctionalProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Functional Property"@en;
        skos:definition "The term that describes Functional Properties."@en;
        skos:broader och:PropertyCharacteristic.

och:ObjectPropertyCharacteristic a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Object Property Characteristic"@en;
        skos:definition "The term that describes those property characteristics that are exclusive to object properties"@en;
        skos:broader och:PropertyCharacteristic;
        skos:narrower och:SymmetricProperty,och:AsymmetricProperty,och:ReflexiveProperty,och:IrreflexiveProperty,och:InverseFunctionalProperty,och:TransitiveProperty.

och:SymmetricProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Symmetric Property"@en;
        skos:definition "The term that describes symmetric properties."@en;
        skos:broader och:PropertyCharacteristic.

och:AsymmetricProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Asymmetric Property"@en;
        skos:definition "The term that describes asymmetric properties."@en;
        skos:broader och:PropertyCharacteristic.       

och:ReflexiveProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Reflexive Property"@en;
        skos:definition "The term that describes reflexive properties."@en;
        skos:broader och:PropertyCharacteristic.

och:IrreflexiveProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Irreflexive Property"@en;
        skos:definition "The term that describes irreflexive properties."@en;
        skos:broader och:PropertyCharacteristic.

och:InverseFunctionalProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Inverse Functional Property"@en;
        skos:definition "The term that describes the inverse functional properties."@en;
        skos:broader och:PropertyCharacteristic.

och:TransitiveProperty a skos:Concept;
        skos:inScheme <https://w3id.org/def/och#>;
        skos:prefLabel "Transitive Property"@en;
        skos:definition "The term that describes transitive properties."@en;
        skos:broader och:PropertyCharacteristic.

